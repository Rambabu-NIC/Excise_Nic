<%@ Page Language="C#" MasterPageFile="~/Site.master" AutoEventWireup="true" CodeBehind="ComplaintStatusReport.aspx.cs" Inherits="ExciseAPI.ECMS.ComplaintStatusReport" %>

<asp:Content ID="Content1" ContentPlaceHolderID="head" runat="Server">
</asp:Content>
<asp:Content ID="Content2" ContentPlaceHolderID="ContentPlaceHolder1" runat="Server">

    <div class="w-100 fl container-fluid">

        <div class="block black bg-white">
            <div class="head">District Wise/All Complaint Report</div>
            <div class="content">
                <div class="form-group col-xs-12 col-sm-12 col-md-12 col-lg-12 p-0 xs-field">
                    <label class="col-xs-4 col-sm-5 col-md-5 col-lg-4" style="text-align: right">District Name</label>
                    <div class="col-xs-4 col-sm-5 col-md-5 col-lg-4">
                        <asp:DropDownList ID="ddldist" runat="server" CssClass="form-control input-b-b"
                            OnSelectedIndexChanged="ddldist_SelectedIndexChanged" AutoPostBack="true">
                        </asp:DropDownList>
                    </div>
                </div>
                <br />
                <div class="content" id="divgridcount" runat="server" visible="false">
                    <asp:GridView ID="grvcount" runat="server" ShowFooter="false" AutoGenerateDeleteButton="false" AutoGenerateColumns="false"
                        EmptyDataText="No Data are available to show" EmptyDataRowStyle-HorizontalAlign="Center"
                        ShowHeaderWhenEmpty="True" FooterStyle-BackColor="#ffcc66"
                        CssClass="table table-striped table-bordered nowrap gvv">
                          <HeaderStyle BackColor="#8ad77c" />
                        <Columns>
                            <%-- <asp:TemplateField HeaderText="SI.No" ItemStyle-CssClass="txt-cnt">
                                <ItemTemplate>
                                    <%#Container.DataItemIndex + 1 %>
                                </ItemTemplate>
                            </asp:TemplateField>--%>
                            <%--<asp:BoundField DataField="New" HeaderText="New" />--%>
                            <asp:TemplateField HeaderText="New">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkNewFilled" runat="server" Text='<%# Bind("New") %>' OnClick="lnkNewFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <%--<asp:BoundField DataField="Unattended" HeaderText="Unattended" />--%>
                            <asp:TemplateField HeaderText="Unattended">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkUnattendedFilled" runat="server" Text='<%# Bind("Unattended") %>' OnClick="lnkUnattendedFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <%--<asp:BoundField DataField="Assigned to DC" HeaderText="Assigned to DC" />--%>
                            <asp:TemplateField HeaderText="Assigned to DC">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkAssignedtoDCFilled" runat="server" Text='<%# Bind("Assigned_to_DC") %>' OnClick="lnkAssignedtoDCFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <%--<asp:BoundField DataField="Assigned to Enquiry Officer" HeaderText="Assigned to Enquiry Officer" />--%>
                            <asp:TemplateField HeaderText="Assigned to Enquiry Officer">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkAssignedtoEnquiryOfficerFilled" runat="server" Text='<%# Bind("Assigned_to_Enquiry_Officer") %>' OnClick="lnkAssignedtoEnquiryOfficerFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <%--<asp:BoundField DataField="Enquiry Completed" HeaderText="Enquiry Completed" />--%>
                            <asp:TemplateField HeaderText="Enquiry Completed">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkEnquiryCompletedFilled" runat="server" Text='<%# Bind("Enquiry_Completed") %>' OnClick="lnkEnquiryCompletedFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <%--<asp:BoundField DataField="Invalid" HeaderText="Invalid" />--%>
                            <asp:TemplateField HeaderText="Invalid">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkInvalidFilled" runat="server" Text='<%# Bind("Invalid") %>' OnClick="lnkInvalidFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <%--<asp:BoundField DataField="Closed" HeaderText="Closed" />--%>
                            <asp:TemplateField HeaderText="Closed">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkClosedFilled" runat="server" Text='<%# Bind("Closed") %>' OnClick="lnkClosedFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <%--<asp:BoundField DataField="Review" HeaderText="Review" />--%>
                            <asp:TemplateField HeaderText="Review">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkReviewFilled" runat="server" Text='<%# Bind("Review") %>' OnClick="lnkReviewFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>
                            <%--<asp:BoundField DataField="ATR Preliminary Submitted" HeaderText="ATR Preliminary Submitted" />--%>
                            <asp:TemplateField HeaderText="ATR Preliminary Submitted">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkATRPreliminarySubmittedFilled" runat="server" Text='<%# Bind("ATR_Preliminary_Submitted") %>' OnClick="lnkATRPreliminarySubmittedFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>

                            <%-- <asp:BoundField DataField="ATR Submitted" HeaderText="ATR Submitted" />--%>
                            <asp:TemplateField HeaderText="ATR Submitted">
                                <ItemTemplate>
                                    <asp:LinkButton ID="lnkATRSubmittedFilled" runat="server" Text='<%# Bind("ATR_Submitted") %>' OnClick="lnkATRSubmittedFilled_Click"></asp:LinkButton>
                                </ItemTemplate>
                            </asp:TemplateField>

                        </Columns>
                        <%-- <HeaderStyle BackColor="#1457a7" Font-Bold="True" ForeColor="White" HorizontalAlign="Center" />--%>
                    </asp:GridView>
                </div>
                <div class="col-xs-12 col-sm-12 txt-cnt">
                    <asp:Label ID="lblError" runat="server" Style="text-align: center"></asp:Label>
                </div>
            </div>
        </div>
    </div>
    <div class="w-100 fl container-fluid" id="divgrid" runat="server" visible="false">
        <div class="block-liner block bg-white">
            <div class="content">
                <div id="divgridwithdate1" runat="server" visible="true">
                    <div runat="server" id="DivGeneratedDate" visible="false">
                        <div class="w-100 fl m-b-2" id="divYearDiv" runat="server" visible="true">
                            <div class="w-100 fl m-b-2">
                                <div class="fr">
                                    <a href="#">
                                        <asp:ImageButton runat="server" ID="btnAvailableImageExportToPdf" ImageUrl="~/Assets/images/PDF.png" OnClick="btnAvailableImageExportToPdf_Click"
                                            Visible="false" />
                                    </a>&nbsp;&nbsp;
                                <a href="#">
                                    <asp:ImageButton runat="server" ID="btnAvailableImageExportToExcel" ImageUrl="~/Assets/images/excel.gif" OnClick="btnAvailableImageExportToExcel_Click"
                                        Visible="false" />
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="w-100 fl container-fluid" id="divgriddetails" runat="server" visible="true">
                        <div class="block-liner block black bg-white">
                            <div class="col-md-12 text-center">
                                <h4 style="text-align: center; font-weight: bold;">
                                    <asp:Label ID="lblstatusreport" runat="server"></asp:Label>&nbsp;&nbsp;Complaints Report</h4>
                            </div>
                            <div class="content">
                                <asp:GridView ID="gvdetails" runat="server" ShowFooter="false" AutoGenerateDeleteButton="false" AutoGenerateColumns="false"
                                    EmptyDataText="No Data are available to show" EmptyDataRowStyle-HorizontalAlign="Center"
                                    ShowHeaderWhenEmpty="True" FooterStyle-BackColor="#ffcc66"
                                    CssClass="table table-striped table-bordered nowrap gvv">
                                      <HeaderStyle BackColor="#8ad77c" />
                                    <Columns>
                                        <asp:TemplateField HeaderText="SI.No" ItemStyle-CssClass="txt-cnt">
                                            <ItemTemplate>
                                                <%#Container.DataItemIndex + 1 %>
                                            </ItemTemplate>
                                        </asp:TemplateField>
                                        <asp:BoundField DataField="ComplaintID" HeaderText="ComplaintID" />
                                        <asp:BoundField DataField="Name" HeaderText="Name" />
                                        <asp:BoundField DataField="Email" HeaderText="Email" />
                                        <asp:BoundField DataField="DistName" HeaderText="District Name" />
                                        <asp:BoundField DataField="MandName" HeaderText="Mandal Name" />
                                        <asp:BoundField DataField="VillName" HeaderText="Village Name" />
                                        <asp:BoundField DataField="Status" HeaderText="Status" />
                                    </Columns>
                                    <%-- <HeaderStyle BackColor="#1457a7" Font-Bold="True" ForeColor="White" HorizontalAlign="Center" />--%>
                                </asp:GridView>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

</asp:Content>


